<% include ('partials/header') %>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/style.css">
    <title>Document</title>
    </head>

    <body>
        <div class="container">
            <h1>Dog Forum</h1>

            <form action="/post" method="post">
                <div>
                    <label for="name">Name of dog</label>
                    <input type="text" class="form-control" id="name" required name="name">
                </div>
                <div>
                    <label for="age">Age of dog</label>
                    <input type="text" class="form-control" id="age" required name="age">
                </div>
                <div>
                    <label for="breed">Breed of dog</label>
                    <input type="text" class="form-control" id="breed" required name="breed">
                </div>
                <div>
                    <label for="height">Height of dog</label>
                    <input type="text" class="form-control" id="height" required name="height">
                </div>
                <div>
                    <label for="weight">Weight of dog</label>
                    <input type="text" class="form-control" id="weight" required name="weight">
                </div>
                <div>
                    <label for="owner">Owner of dog</label>
                    <input type="text" class="form-control" id="owner" required name="owner">
                </div>
                <button type="submit" class="btn btn-info">Sumbit</button>
            </form>
        </div>

    </body>

    </html>